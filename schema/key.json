{
    "$schema": "http://json-schema.org/schema#",
    "title": "key",
    "description": "`Key` objects describe key-value pairs in each entry.",
    "version": "0.5",
    "type": "object",

    "properties": {
        "type": {
            "description": "the data type for the corresponding stored value.",
            "unique": false,
            "distinguishing": true,
            "default": null
        },
        "unique": {
            "description": "If an attribute is `unique`, then any other instance with the same value refers to the same object (i.e. is a duplicate).",
            "type": "boolean",
            "unique": false,
            "distinguishing": true,
            "default": false
        },
        "distinguishing": {
            "description": "If an attribute is `distinguishing`, then if its value is different from another instance, they are not the same objects (i.e. not a duplicate).",
            "type": "boolean",
            "unique": false,
            "distinguishing": true,
            "default": true
        },

        "no_source": {
            "type": "boolean",
            "unique": false,
            "distinguishing": true,
            "default": false
        },
        "priority": {
            "type": "integer",
            "unique": false,
            "distinguishing": true,
            "default": 0
        },
        "kind_preference": {
            "type": "array",
            "unique": false,
            "distinguishing": true,
            "default": [],
            "items": {
                "type": "string"
            }
        },
        "replace_better": {
            "type": "boolean",
            "unique": false,
            "distinguishing": true,
            "default": false
        }

    }

}
