{
    "$schema": "http://json-schema.org/schema#",
    "title": "key",
    "description": "`Key` objects describe key-value pairs in each entry.",
    "version": "0.4",
    "type": "object",

    "properties": {
        "type": {
            "type": ["string", "array"],
            "unique": true
        },
        "unique": {
            "type": "boolean",
            "unique": true
        }
    },

    "required": ["type", "unique"]

}
